{
  "expo": {
    "name": "swiftly_app",
    "slug": "swiftly",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "scheme": "myapp",
    "orientation": "portrait",
    "icon": "./assets/swiftly_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/swiftly_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sebmicsep.swiftly",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sebmicsep.swiftly"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "c106b9bb-0949-49aa-a555-f4206d01b7a7"
      }
    },
    "owner": "sebmicsep",
    "plugins": [
      "expo-router",
      "expo-apple-authentication"
    ]
  }
}
